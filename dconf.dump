[org/gnome/desktop/background]
picture-uri='file:///home/lnqs/Pictures/Wallpapers/circuits-hearts_00428111.jpg'

[org/gnome/desktop/input-sources]
sources=[('xkb', 'us+altgr-intl')]
xkb-options=['lv3:ralt_switch']

[org/gnome/desktop/interface]
document-font-name='Roboto 10'
enable-hot-corners=false
font-name='Roboto 10'
gtk-im-module='gtk-im-context-simple'
gtk-theme='Adwaita-dark'
monospace-font-name='Roboto Mono 10'

[org/gnome/desktop/media-handling]
autorun-never=true

[org/gnome/desktop/peripherals/touchpad]
click-method='areas'
natural-scroll=false
two-finger-scrolling-enabled=true

[org/gnome/desktop/search-providers]
disable-external=true
sort-order=['org.gnome.Contacts.desktop', 'org.gnome.Documents.desktop', 'org.gnome.Nautilus.desktop']

[org/gnome/desktop/sound]
allow-volume-above-100-percent=true

[org/gnome/desktop/wm/preferences]
focus-mode='sloppy'
resize-with-right-button=true
titlebar-font='Roboto 10'

[org/gnome/mutter]
workspaces-only-on-primary=false

[org/gnome/settings-daemon/plugins/media-keys]
custom-keybindings=['/org/gnome/settings-daemon/plugins/media-keys/custom-keybindings/custom0/']

[org/gnome/settings-daemon/plugins/media-keys/custom-keybindings/custom0]
binding='<Super>Return'
command='kitty'
name='Kitty'

[org/gnome/settings-daemon/plugins/power]
power-button-action='interactive'

[org/gnome/shell]
enabled-extensions=['arch-update@RaphaelRochet', 'appindicatorsupport@rgcjonas.gmail.com', 'launch-new-instance@gnome-shell-extensions.gcampax.github.com', 'native-window-placement@gnome-shell-extensions.gcampax.github.com', 'drive-menu@gnome-shell-extensions.gcampax.github.com', 'system-monitor@paradoxxx.zero.gmail.com', 'user-theme@gnome-shell-extensions.gcampax.github.com', 'Hide_Activities@shay.shayel.org', 'pixel-saver@deadalnix.me', 'gsconnect@andyholmes.github.io', 'syncthingicon@jay.strict@posteo.de']

[org/gnome/shell/extensions/arch-update]
always-visible=false
notify=true
update-cmd="kitty -e 'sh -c  \"sudo yay -Syu ; echo Done - Press enter to exit; read\" '"

[org/gnome/shell/extensions/system-monitor]
cpu-display=true
cpu-show-menu=true
cpu-show-text=true
cpu-style='digit'
disk-style='digit'
fan-style='digit'
freq-display=false
freq-style='digit'
gpu-style='digit'
icon-display=false
memory-style='digit'
net-style='digit'
thermal-style='digit'

[org/gnome/shell/extensions/user-theme]
name='lnqs'

[system/locale]
region='de_DE.UTF-8'
